{
  "astroBotConfig":{
    "responseTimeout":5250,
    "openNotifyConfig":{
      "openNotifyApiRootUrl":"http://api.open-notify.org/",
      "astronautsCacheTtl":86400000
    },
    "launchLibraryConfig":{
      "launchLibraryApiRootUrl":"https://launchlibrary.net",
      "launchLibraryVersion":"1.2",
      "upcomingLaunchesToRetrieve":100,
      "agenciesToRetrieve":500,
      "launchesCacheTtl":21600000,
      "agenciesCacheTtl":604800000,
      "ignoreUnknownJsonProperties":true
    },
    "geonamesConfig":{
      "geonamesApiRootUrl":"http://api.geonames.org",
      "username":"derpgroup"
    }
  },
  "ignoreUnknownJsonProperties":"true",
  "server": {
    "applicationConnectors": [
      {
        "type": "http",
        "port" : 11080
      }
    ],
    "adminConnectors": [
      {
        "type": "http",
        "port": 11081
      }
    ]
  },
  "logging": {
    "level": "INFO",
    "loggers": {
      "com.derpgroup": {
        "level": "INFO",
        "appenders": [
          {
            "type": "file",
            "currentLogFilename": "~/testoutput/astrobot.log",
            "archivedLogFilenamePattern": "~/testoutput/astrobot-%d.log",
            "archivedFileCount": 5
          }
        ]
      }
    }
  }
}
